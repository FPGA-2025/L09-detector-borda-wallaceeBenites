    %store/vec4 v0x626e38a2d7d0_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x626e38a2d7d0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x626e38a2d6c0_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x626e38a2d6c0_0;
