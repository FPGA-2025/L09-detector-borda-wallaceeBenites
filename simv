v0x626e38a2cf50_0 .net "clk", 0 0, v0x626e38a2d420_0;  1 drivers
v0x626e38a2d010_0 .net "detector", 1 0, L_0x626e38a2d8d0;  alias, 1 drivers
v0x626e38a2d0d0_0 .net "entrada", 1 0, v0x626e38a2d620_0;  1 drivers
v0x626e38a2d1b0_0 .var "entrada_reg", 1 0;
v0x626e38a2d2e0_0 .net "rst", 0 0, v0x626e38a2d7d0_0;  1 drivers
E_0x626e389dd1f0 .event posedge, v0x626e38a2d2e0_0, v0x626e38a2cf50_0;
    .scope S_0x626e38a176d0;
T_0 ;
    %wait E_0x626e389dd1f0;
    %load/vec4 v0x626e38a2d2e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x626e38a2d1b0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x626e38a2d0d0_0;
    %assign/vec4 v0x626e38a2d1b0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x626e38a17540;
T_1 ;
    %delay 1, 0;
    %load/vec4 v0x626e38a2d420_0;
    %pushi/vec4 0, 0, 1;
    %cmp/e;
    %flag_get/vec4 6;
    %store/vec4 v0x626e38a2d420_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x626e38a17540;
T_2 ;
    %vpi_call 2 24 "$readmemb", "teste.txt", v0x626e38a2d4e0 {0 0 0};
    %vpi_call 2 26 "$dumpfile", "saida.vcd" {0 0 0};
    %vpi_call 2 27 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x626e38a17540 {0 0 0};
    %vpi_call 2 30 "$monitor", "entrada[0]=%b, detector[0]=%b, entrada[1]=%b, detector[1]=%b", &PV<v0x626e38a2d620_0, 0, 1>, &PV<v0x626e38a2d580_0, 0, 1>, &PV<v0x626e38a2d620_0, 1, 1>, &PV<v0x626e38a2d580_0, 1, 1> {0 0 0};
