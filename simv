    %ix/getv/s 4, v0x626e38a2d6c0_0;
    %load/vec4a v0x626e38a2d4e0, 4;
    %store/vec4 v0x626e38a2d620_0, 0, 2;
    %delay 2, 0;
    %load/vec4 v0x626e38a2d6c0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x626e38a2d6c0_0, 0, 32;
    %jmp T_2.0;
T_2.1 ;
    %delay 2, 0;
    %vpi_call 2 40 "$finish" {0 0 0};
    %end;
    .thread T_2;
